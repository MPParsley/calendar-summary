{
  "name": "cultuurnet/calendar-summary",
  "description": "Library to convert a cdbXML input to a readable summary",
  "license": "Apache-2.0",
  "require": {
    "php": ">=5.6.0",
    "ext-intl": "*",
    "cultuurnet/cdb": "~2.2.0"
  },
  "autoload": {
    "psr-4": {
      "CultuurNet\\CalendarSummary\\": ["src"]
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.x-dev"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "~5.7",
    "squizlabs/php_codesniffer": "~2.3",
    "phing/phing": "~2.11",
    "satooshi/php-coveralls": "~0.7"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
